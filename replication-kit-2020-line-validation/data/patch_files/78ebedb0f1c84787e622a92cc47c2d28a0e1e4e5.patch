+        final byte[] buf = new byte[size];
+        generator.nextBytes(buf, -1, num);
+    }
+    @Test(expected=OutOfRangeException.class)
+    public void testPreconditionNextBytes2() {
+        final int size = 10;
+        final byte[] buf = new byte[size];
+        generator.nextBytes(buf, size, 0);
