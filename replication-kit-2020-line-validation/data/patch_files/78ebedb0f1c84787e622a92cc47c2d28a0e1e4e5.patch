+        if (mti >= N) { // Generate N words at one time.
+            int mtNext = mt[0];
+            for (int k = 0; k < N - M; ++k) {
+                int mtCurr = mtNext;
+                mtNext = mt[k + 1];
+                y = (mtCurr & UPPER_MASK) | (mtNext & LOWER_MASK);
