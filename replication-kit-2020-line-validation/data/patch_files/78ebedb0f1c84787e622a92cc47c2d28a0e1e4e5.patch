+     * Initialize the internal state of this instance.
+     *
+     * @param seed Seed.
+     */
+    private void initState(int seed) {
+        long longMT = seed & INT_MASK_LONG;
+        mt[0]= (int) longMT;
+        for (mti = 1; mti < N; ++mti) {
+            longMT = (1812433253L * (longMT ^ (longMT >> 30)) + mti) & INT_MASK_LONG;
