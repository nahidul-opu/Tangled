From fe3a4ece1438845504c67584da3a7b36fdb5372f Mon Sep 17 00:00:00 2001
From: Sebastian Bazley <sebb@apache.org>
Date: Tue, 17 Mar 2009 16:11:28 +0000
Subject: [PATCH] VFS-242 NamingTests - assertSameName invokes
 relName.replace('\\', '/'); without using return value

git-svn-id: https://svn.apache.org/repos/asf/commons/proper/vfs/trunk@755295 13f79535-47bb-0310-9956-ffa450edef68
---
 .../java/org/apache/commons/vfs/test/NamingTests.java  | 10 ++++++----
 1 file changed, 6 insertions(+), 4 deletions(-)

diff --git a/core/src/test/java/org/apache/commons/vfs/test/NamingTests.java b/core/src/test/java/org/apache/commons/vfs/test/NamingTests.java
index a8993d8012..2347a9d6b8 100644
--- a/core/src/test/java/org/apache/commons/vfs/test/NamingTests.java
+++ b/core/src/test/java/org/apache/commons/vfs/test/NamingTests.java
@@ -226,14 +226,16 @@ private void assertSameName(final String expectedPath,
         FileName name = getManager().resolveName(baseName, relName, scope);
         assertEquals(expectedPath, name.getPath());
 
+        String temp;
+        
         // Replace the separators
-        relName.replace('\\', '/');
-        name = getManager().resolveName(baseName, relName, scope);
+        temp = relName.replace('\\', '/');
+        name = getManager().resolveName(baseName, temp, scope);
         assertEquals(expectedPath, name.getPath());
 
         // And again
-        relName.replace('/', '\\');
-        name = getManager().resolveName(baseName, relName, scope);
+        temp = relName.replace('/', '\\');
+        name = getManager().resolveName(baseName, temp, scope);
         assertEquals(expectedPath, name.getPath());
     }
 
