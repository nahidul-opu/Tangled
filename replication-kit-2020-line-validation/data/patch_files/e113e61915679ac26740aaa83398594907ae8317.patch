 .../org/apache/commons/dbcp/managed/ManagedConnection.java    | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/src/java/org/apache/commons/dbcp/managed/ManagedConnection.java b/src/java/org/apache/commons/dbcp/managed/ManagedConnection.java
index e6d5537806..14bba8dcdc 100644
--- a/src/java/org/apache/commons/dbcp/managed/ManagedConnection.java
+++ b/src/java/org/apache/commons/dbcp/managed/ManagedConnection.java
@@ -153,8 +153,8 @@ public void close() throws SQLException {
     }
 
     protected class CompletionListener implements TransactionContextListener {
-        public void afterCompletion(TransactionContext transactionContext, boolean commited) {
-            if (transactionContext == transactionContext) {
+        public void afterCompletion(TransactionContext completedContext, boolean commited) {
+            if (completedContext == transactionContext) {
                 transactionComplete();
             }
         }
